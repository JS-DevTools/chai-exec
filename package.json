{
  "name": "chai-exec",
  "version": "0.0.1",
  "description": "Chai assertions for testing your CLI",
  "keywords": [
    "chai",
    "chai-plugin",
    "cli",
    "spawn",
    "exec"
  ],
  "author": {
    "name": "Randall Krauskopf",
    "url": "http://github.com/rkrauskopf"
  },
  "contributors": [
    "Randall Krauskopf (http://github.com/rkrauskopf)",
    "James Messinger (https://jamesmessinger.com)"
  ],
  "license": "MIT",
  "homepage": "https://jsdevtools.org/chai-exec",
  "repository": {
    "type": "git",
    "url": "https://github.com/JS-DevTools/chai-exec.git"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint lib test",
    "test": "mocha && npm run lint",
    "cover": "istanbul cover _mocha",
    "upgrade": "npm-check -u",
    "bump": "bump --prompt --tag --push --all",
    "release": "npm run upgrade && npm test && npm run bump && npm publish"
  },
  "devDependencies": {
    "chai": "^4.0.1",
    "codacy-coverage": "^2.0.1",
    "coveralls": "^2.11.14",
    "eslint": "^3.15.0",
    "eslint-config-modular": "^4.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "npm-check": "^5.4.0",
    "version-bump-prompt": "^3.0.1"
  },
  "dependencies": {
    "ez-spawn": "^1.0.0"
  },
  "engines": {
    "node": ">=4"
  }
}
